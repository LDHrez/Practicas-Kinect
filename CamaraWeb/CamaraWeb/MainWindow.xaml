<Window x:Class="CamaraWeb.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CamaraWeb"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Loaded="windows">
    <Grid>
        <Image x:Name="mostrarVideo" HorizontalAlignment="Left" Height="299" Margin="10,10,0,0" VerticalAlignment="Top" Width="497"/>
        <Button x:Name="btnCaptura" Content="Capturar" HorizontalAlignment="Left" Margin="416,283,0,0" VerticalAlignment="Top" Width="91" Height="26" BorderBrush="#FFFF0606" Background="#FFFF0707" Click="btnCaptura_Click"/>
        <Button x:Name="btnReset" Content="Limpiar" HorizontalAlignment="Left" Margin="416,226,0,0" VerticalAlignment="Top" Width="91" Height="31" BorderBrush="#FF09D11B" Background="#FF38C901" Click="btnReset_Click"/>

    </Grid>
</Window>
